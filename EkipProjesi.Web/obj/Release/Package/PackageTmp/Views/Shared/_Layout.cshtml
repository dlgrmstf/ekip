        <!-- START: Main Menu-->
        <div class="sidebar">
            <div class="site-width">
                @{ Html.RenderAction("LeftMenu", "Home");}
            </div>
        </div>
        <!-- END: Main Menu-->
        <!-- START: Content-->
        <main>
            <div class="container-fluid site-width">
                @RenderBody()
            </div>
        </main>
        <!-- END: Content-->
        <br />
    </div>
    <!-- START: Footer-->
    <footer class="site-footer">
        © @DateTime.Now.Year <a href="https://www.ordinatrum.com.tr" target="_blank">Ordinatrum Bilişim Teknolojileri ve Danışmanlık</a>
    </footer>
    <!-- END: Footer-->
    <!-- START: Back to top-->
    <a href="#" class="scrollup text-center">
        <i class="icon-arrow-up"></i>
    </a>
    <!-- END: Back to top-->
