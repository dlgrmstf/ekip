                                            <option value="@((int)GorusmeMerkezi.Erenkoy)">Erenköy BAHAR</option>
                                            <option value="@((int)GorusmeMerkezi.Uskudar)">Üsküdar BAHAR</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-6">
