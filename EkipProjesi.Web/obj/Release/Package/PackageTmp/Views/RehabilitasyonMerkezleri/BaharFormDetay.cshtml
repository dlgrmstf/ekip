    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="https://code.jquery.com/jquery-1.9.1.min.js"></script>
<link href="~/Content/global/vendor/bootstrap-select/bootstrap-select.css" rel="stylesheet" />
<script src="~/Content/global/vendor/bootstrap-select/bootstrap-select.js"></script>

<div class="row">
    <div class="col-12  align-self-center">
        <div class="sub-header mt-3 py-3 align-self-center d-sm-flex w-100 rounded">
            <div class="w-sm-100 mr-auto"><h4 class="mb-0">Form Detay</h4> <a href="@Url.Action("Index", "RehabilitasyonMerkezleri")"><p>Anasayfa</p></a></div>
            <ol class="breadcrumb bg-transparent align-self-center m-0 p-0">
                <li class="breadcrumb-item"><a href="#">Form Detay</a></li>
                <li class="breadcrumb-item active">Anasayfa</li>
            </ol>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-12 mt-1">
        <div class="position-relative">
            <div class="position-relative px-1 py-3" style="background-color: #1e3d73">
                <div class="media d-md-flex d-block">
                    <a href="#"><img src="~/images/user.png" width="100" height="100" alt="" class="img-fluid rounded-circle" style="margin-left:10px"></a>
                    <div class="media-body z-index-1">
                        <div class="pl-4">
                            <h3 class="display-4 text text-white mb-0">@ViewBag.HastaAdi @ViewBag.HastaSoyadi</h3>
                            <h6 class="text text-white mb-0">TC Kimlik No: @ViewBag.HastaTC</h6>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <br />
    </div>
</div>
<div class="row">
    <div class="col-12 mt-4">
        <div class="card">
            <div class="card-header">
                <h4 class="card-title">Rehabilitasyon Değerlendirme Formu</h4>
            </div>
            <div class="card-content">
                <div class="card-body">
                    <div class="row">
                        <div class="col-12">
                            <form class="form-horizontal" id="form" role="form">
                                <div class="form-row">
                                    <div class="form-group col-md-6" hidden="hidden">
                                        <label for="ID">Hasta Adı:</label>
                                        <input type="text" class="form-control" id="@Html.IdFor(x => x.HastaID)" name="@Html.NameFor(x => x.HastaID)" value="@ViewBag.VakaID">
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="hastaadi">Hasta Adı:</label>
                                        <input type="text" class="form-control" value="@ViewBag.HastaAdi" disabled>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="hastasoyadi">Hasta Soyadı:</label>
                                        <input type="text" class="form-control" value="@ViewBag.HastaSoyadi" disabled>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="calisma">Halen/Şu an çalışıyor mu?</label>
                                        <select id="@Html.IdFor(x => x.CalismaDurumu)" name="@Html.NameFor(x => x.CalismaDurumu)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)BaharCalismaDurumu.Hayir)">Hayır</option>
                                            <option value="@((int)BaharCalismaDurumu.Duzensiz)">Düzensiz olarak çalışıyor</option>
                                            <option value="@((int)BaharCalismaDurumu.Duzenli)">Düzenli olarak çalışıyor</option>
                                            <option value="@((int)BaharCalismaDurumu.Emekli)">Emekli</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="calisma">Ekonomik durumunu nasıl tanımlıyor?</label>
                                        <select id="@Html.IdFor(x => x.EkonomikDurumu)" name="@Html.NameFor(x => x.EkonomikDurumu)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)BaharEkonomikDurum.Iyi)">İyi</option>
                                            <option value="@((int)BaharEkonomikDurum.Orta)">Orta</option>
                                            <option value="@((int)BaharEkonomikDurum.Kotu)">Kötü</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="yasadigiil">Yaşadığı İl:</label>
                                        <input type="text" class="form-control" id="@Html.IdFor(x => x.YasadigiIl)" name="@Html.NameFor(x => x.YasadigiIl)" maxlength="50" value="@Model.YasadigiIl" disabled>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="yasadigiilce">Yaşadığı İlçe:</label>
                                        <input type="text" class="form-control" id="@Html.IdFor(x => x.YasadigiIlce)" name="@Html.NameFor(x => x.YasadigiIlce)" maxlength="50" value="@Model.YasadigiIlce" disabled>
                                    </div>
                                    <div class="form-group col-md-6">
