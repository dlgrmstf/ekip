@model EkipProjesi.Core.ArindirmaModulu.IArindirmaPersonelBilgileriFiltreModel

<script src="https://code.jquery.com/jquery-1.9.1.min.js"></script>
<link rel="stylesheet" href="~/dist/vendors/datatable/css/dataTables.bootstrap4.min.css" />
<link rel="stylesheet" href="~/dist/vendors/datatable/buttons/css/buttons.bootstrap4.min.css" />

<div class="row">
    <div class="col-12 mt-3">
        <div class="card">
            <div class="card-header">
                <h4>
                    PERSONEL BİLGİLERİ İSTATİSTİK
                </h4>
            </div>
            @if (Model.PersonelBilgileri != null)
            {
                <div class="card-body">
                    <div class="">
                        <div class="">
                            <div class="table-responsive">
                                <table id="example1" class="display table dataTable table-striped table-bordered" width="99%">
                                    <thead>
                                        <tr>
                                            <th>ID</th>
                                            <th>Kayıt Tarihi</th>
                                            <th>Tüm Uzman Hekim Sayısı</th>
                                            <th>Tüm Asistan Hekim Sayısı</th>
                                            <th>Tüm Pratisyen Hekim Sayısı</th>
                                            <th>Aktif Psikiyatri Uzmanı Sayısı</th>
                                            <th>Çocuk Psikiyatri Uzmanı Sayısı</th>
                                            <th>Psikiyatri Asistanı Sayısı</th>
                                            <th>Psikiyatri Asistanı Sayısı</th>
                                            <th>Çocuk Psikiyatri Asistanı Sayısı</th>
                                            <th>Psikolog Sayısı</th>
                                            <th>Hemşire Sayısı</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model.PersonelBilgileri)
                                        {
                                            <tr>
                                                <td>@item.ID</td>
                                                <td>@item.KayitTarihi.ToString("dd/MM/yyyy")</td>
                                                <td>@item.TumUzmanHekimSayisi</td>
                                                <td>@item.TumAsistanHekimSayisi</td>
                                                <td>@item.TumPratisyenHekimSayisi</td>
                                                <td>@item.HastaneAktifPsikiyatriUzmaniSayisi</td>
                                                <td>@item.HastaneAktifCocukPsikiyatriUzmaniSayisi</td>
                                                <td>@item.HastaneAktifPsikiyatriAsistaniSayisi</td>
                                                <td>@item.HastaneAktifPsikiyatriAsistaniSayisi</td>
                                                <td>@item.HastaneAktifCocukPsikiyatriAsistaniSayisi</td>
                                                <td>@item.HastaneAktifPsikologSayisi</td>
                                                <td>@item.HastaneAktifHemsireSayisi</td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            }
            else
            {
                <div class="card-body">
                    <p>Aradığınız kriterlere uygun sonuç bulunamadı.</p>
                </div>
            }
        </div>
    </div>
</div>
