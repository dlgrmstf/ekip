@model EkipProjesi.Core.ArindirmaModulu.IArindirmaHizmetBilgileriFiltreModel

<link rel="stylesheet" href="~/dist/vendors/c3/c3.min.css">

<script src="https://code.jquery.com/jquery-1.9.1.min.js"></script>
<link rel="stylesheet" href="~/dist/vendors/datatable/css/dataTables.bootstrap4.min.css" />
<link rel="stylesheet" href="~/dist/vendors/datatable/buttons/css/buttons.bootstrap4.min.css" />

<div class="row">
    <div class="col-12 mt-3">
        <div class="card">
            <div class="card-header">
                <a href="~/Views/ArindirmaMerkezleri/PartialFiltreSonuc.cshtml"></a>
                <h4>
                    AMATEM İSTATİSTİK
                </h4>
            </div>
            @if (Model.HizmetBilgileri != null)
            {
                <div class="card-body">
                    <div class="">
                        <div class="">
                            <div class="table-responsive">
                                <table id="example1" class="display table dataTable table-striped table-bordered" width="99%">
                                    <thead>
                                        <tr>
                                            <th>ID</th>
                                            <th>Kayıt Tarihi</th>
                                            <th>Aktif Amatem Poliklinik Sayısı</th>
                                            <th>Amatem Yeni Başvuru Sayısı</th>
                                            <th>Amatem Takip Başvurusu Sayısı</th>
                                            <th>Aktif Amatem Yatak Sayısı</th>
                                            <th>Amatem Yatak Doluluk Oranı %</th>
                                            <th>Amatem Boş Yatak Sayısı</th>
                                            <th>Amatem Dolu Yatak Sayısı</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model.HizmetBilgileri)
                                        {
                                            <tr>
                                                <td>@item.ID</td>
                                                <td>@item.KayitTarihi.ToString("dd/MM/yyyy")</td>
                                                <td>@item.AktifAmatemPoliklinikSayisi</td>
                                                <td>@item.AmatemYeniBasvuruSayisi</td>
                                                <td>@item.AmatemTakipBasvurusuSayisi</td>
                                                <td>@item.AktifAmatemYatakSayisi</td>
                                                <td>@item.AmatemYatakDolulukOrani </td>
                                                <td>@item.AmatemBosYatakSayisi</td>
                                                <td>@item.AmatemDoluYatakSayisi</td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            }
            else
            {
                <div class="card-body">
                    <p>Aradığınız kriterlere uygun sonuç bulunamadı.</p>
                </div>
            }
        </div>
    </div>
</div>
