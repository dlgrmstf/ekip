@model EkipProjesi.Core.RehabilitasyonMerkezleri.IHastaBagimlilikSiddetiFormlari
@{
    ViewBag.Title = "OlceklerFormDetay";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="https://code.jquery.com/jquery-1.9.1.min.js"></script>
<link href="~/Content/global/vendor/bootstrap-select/bootstrap-select.css" rel="stylesheet" />
<script src="~/Content/global/vendor/bootstrap-select/bootstrap-select.js"></script>

<div class="row">
    <div class="col-12  align-self-center">
        <div class="sub-header mt-3 py-3 align-self-center d-sm-flex w-100 rounded">
            <div class="w-sm-100 mr-auto"><h4 class="mb-0">Form Detay</h4> <a href="@Url.Action("Index", "RehabilitasyonMerkezleri")"><p>Anasayfa</p></a></div>
            <ol class="breadcrumb bg-transparent align-self-center m-0 p-0">
                <li class="breadcrumb-item"><a href="#">Form Detay</a></li>
                <li class="breadcrumb-item active">Anasayfa</li>
            </ol>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-12 mt-1">
        <div class="position-relative">
            <div class="position-relative px-1 py-3" style="background-color: #1e3d73">
                <div class="media d-md-flex d-block">
                    <a href="#"><img src="~/images/user.png" width="100" height="100" alt="" class="img-fluid rounded-circle" style="margin-left:10px"></a>
                    <div class="media-body z-index-1">
                        <div class="pl-4">
                            <h3 class="display-4 text text-white mb-0">@ViewBag.HastaAdi @ViewBag.HastaSoyadi</h3>
                            <h6 class="text text-white mb-0">TC Kimlik No: @ViewBag.HastaTC</h6>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <br />
    </div>
</div>
<div class="row">
    <div class="col-12 mt-4">
        <div class="card">
            <div class="card-header">
                <h4 class="card-title">Bağımlılık Şiddeti İndeksi</h4>
            </div>
            <div class="card-content">
                <div class="card-body">
                    <div class="row">
                        <div class="col-12">
                            <form class="form-horizontal" id="form" role="form">
                                <h5>Genel Bilgiler</h5>
                                <div class="form-row">
                                    <div class="form-group col-md-3" hidden="hidden">
                                        <label for="ID">Hasta ID:</label>
                                        <input type="text" class="form-control" id="@Html.IdFor(x => x.HastaID)" name="@Html.NameFor(x => x.HastaID)" value="@ViewBag.VakaID">
                                    </div>
                                    <div class="form-group col-md-3">
                                        <label for="hastaadi">Hasta Adı:</label>
                                        <input type="text" class="form-control" value="@ViewBag.HastaAdi" disabled>
                                    </div>
                                    <div class="form-group col-md-3">
                                        <label for="hastasoyadi">Hasta Soyadı:</label>
                                        <input type="text" class="form-control" value="@ViewBag.HastaSoyadi" disabled>
                                    </div>
                                    <div class="form-group col-md-3">
                                        <label>Hasta No:</label>
                                        <input type="text" class="form-control" id="@Html.IdFor(x => x.HastaNo)" name="@Html.NameFor(x => x.HastaNo)" maxlength="10" value="@Model.HastaNo" disabled>
                                    </div>
                                    <div class="form-group col-md-3">
                                        <label>Ülke</label>
                                        <select id="@Html.IdFor(x => x.Ulke)" name="@Html.NameFor(x => x.Ulke)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="1">Australia</option>
                                            <option value="2">Brazil</option>
                                            <option value="3">Canada</option>
                                            <option value="4">China</option>
                                            <option value="5">Colombia</option>
                                            <option value="6">Egypt</option>
                                            <option value="7">Germany</option>
                                            <option value="8">India</option>
                                            <option value="9">Indonesia</option>
                                            <option value="10">Iran</option>
                                            <option value="11">Kazakhstan</option>
                                            <option value="12">Kenya</option>
                                            <option value="13">Mexico</option>
                                            <option value="14">Nigeria</option>
                                            <option value="15">Russia</option>
                                            <option value="16">Spain</option>
                                            <option value="17">Sweden</option>
                                            <option value="18">United Kingdom</option>
                                            <option value="19">USA</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-3">
                                        <label>Merkez</label>
                                        <select id="@Html.IdFor(x => x.Merkez)" name="@Html.NameFor(x => x.Merkez)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="1">Turning Point Alcohol and Drug Centre Inc., Australia</option>
                                            <option value="2">
                                                Psychosocial Attention Centre for Alcohol and other
                                                Drugs, Brazil
                                            </option>
                                            <option value="3">Centre for Addiction and Mental Health CAMH, Canada</option>
                                            <option value="4">Shanghai Drug Abuse Treatment Centre, China</option>
                                            <option value="5">
                                                Carisma Centre for Attention and Integral Mental Health,
                                                Colombia
                                            </option>
                                            <option value="6">General Secretariat of Mental Health, Egypt</option>
                                            <option value="7">Mudra, Germany</option>
                                            <option value="8">TT Ranganathan Clinical Research Foundation, India</option>
                                            <option value="9">
                                                RS Ketergantungan Obat The Drug Dependence Hospital,
                                                Indonesia
                                            </option>
                                            <option value="10">
                                                Iranian National Prison Organisation /Iranian National
                                                Centre for Addiction Studies INCAS, Iran
                                            </option>
                                            <option value="11">
                                                National Research and Clinical Centre on Medical and
                                                Social Problems of Drug, Kazakhstan
                                            </option>
                                            <option value="12">Drug Rehabilitation Unit, Mathari Hospital, Kenya</option>
                                            <option value="13">Centros de Integración Juvenil A.C., Mexico</option>
                                            <option value="14">Neuropsychiatric Hospital Aro, Nigeria</option>
                                            <option value="15">
                                                Regional Research Centre of Narcology and
                                                Psychopharmacology affiliated to St. Petersburg Pavlov
                                                State Medical University, Russia
                                            </option>
                                            <option value="16">Asociación Proyecto Hombre, Spain</option>
                                            <option value="17">Maria Ungdom, Sweden</option>
                                            <option value="18">Cranstoun Drug Services, United Kingdom</option>
                                            <option value="19">Fayette Companies, U.S.A.</option>
                                            <option value="20">
                                                Stanley Street Treatment & Resources (SSTAR) Inc.,
                                                U.S.A.
                                            </option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-3">
                                        <label>Modalite</label>
                                        <select id="@Html.IdFor(x => x.Modalite)" name="@Html.NameFor(x => x.Modalite)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)G2cTedaviKodlari.Bir)">Ayaktan tedavi (haftada 5 saatten az)</option>
                                            <option value="@((int)G2cTedaviKodlari.Iki)">Yoğun ayaktan tedavi (haftada ≥ 5 saat)</option>
                                            <option value="@((int)G2cTedaviKodlari.Uc)">Yatılı/Serviste yatırılarak</option>
                                            <option value="@((int)G2cTedaviKodlari.Dort)">Grup terapisi</option>
                                            <option value="@((int)G2cTedaviKodlari.Bes)">Rehabilitasyon merkezi</option>
                                            <option value="@((int)G2cTedaviKodlari.Alti)">Detoks – Serviste yatırılarak (tipik olarak 3-7 gün)</option>
                                            <option value="@((int)G2cTedaviKodlari.Yedi)">Detoks Ayaktan izlem/Ambulatuvar</option>
                                            <option value="@((int)G2cTedaviKodlari.Sekiz)">Opioid replasmanı, ayaktan (Metadon, Buprenorfin, vb.)</option>
                                            <option value="@((int)G2cTedaviKodlari.Dokuz)">Diğer (düşük eşikli, aile hekimi , ruhani destek, vb.)</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="telefon">Modalite Diğer Açıklama:</label>
                                        <textarea class="form-control" rows="1" cols="15" id="@Html.IdFor(x=>x.ModaliteAciklama)" name="@Html.NameFor(x=>x.ModaliteAciklama)" maxlength="100"></textarea>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>Yatış Tarihi</label>
                                        <input required type="text" class="form-control form-control-sm validate[required]" id="@Html.IdFor(x=>x.BasvuruTarihi)" name="@Html.NameFor(x => x.BasvuruTarihi)"  value="@Model.BasvuruTarihi.ToString("dd/MM/yyyy")" disabled>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>Görüşme Tarihi</label>
                                        <input required type="text" class="form-control form-control-sm validate[required]" id="@Html.IdFor(x=>x.GorusmeTarihi)" name="@Html.NameFor(x => x.GorusmeTarihi)"  value="@Model.GorusmeTarihi.ToString("dd/MM/yyyy")" disabled>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>Başlama Saati</label>
                                        <input required type="time" class="form-control form-control-sm validate[required]" id="@Html.IdFor(x=>x.BaslamaSaati)" name="@Html.NameFor(x => x.BaslamaSaati)"  value="@Model.BaslamaSaati" disabled>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>Bitiş Saati</label>
                                        <input required type="time" class="form-control form-control-sm validate[required]" id="@Html.IdFor(x=>x.BitisSaati)" name="@Html.NameFor(x => x.BitisSaati)"  value="@Model.BitisSaati" disabled>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>Sınıf</label>
                                        <select id="@Html.IdFor(x => x.Sinif)" name="@Html.NameFor(x => x.Sinif)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)BSISinif.Ilk)">İlk Başvuru</option>
                                            <option value="@((int)BSISinif.Takip)">Takip</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>İletişim Şekli</label>
                                        <select id="@Html.IdFor(x => x.IletisimSekli)" name="@Html.NameFor(x => x.IletisimSekli)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)BSIIletisimSekli.Yuz)">Yüz yüze</option>
                                            <option value="@((int)BSIIletisimSekli.Telefon)">Telefon</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>Cinsiyet<br /><br /></label>
                                        <select id="@Html.IdFor(x => x.Cinsiyet)" name="@Html.NameFor(x => x.Cinsiyet)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)Cinsiyet.Erkek)">Erkek</option>
                                            <option value="@((int)Cinsiyet.Kadın)">Kadın</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>Görüşmeci Ad-Soyad ilk harfi:</label>
                                        <input type="text" class="form-control" id="@Html.IdFor(x => x.G11Soru)" name="@Html.NameFor(x => x.G11Soru)" maxlength="10"  value="@Model.G11Soru" disabled>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>Ne kadar zamandır bu adreste yaşıyorsunuz? (Yıl)</label>
                                        <input type="number" class="form-control" id="@Html.IdFor(x => x.G14Yil)" name="@Html.NameFor(x => x.G14Yil)"  value="@Model.G14Yil" disabled>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>Ne kadar zamandır bu adreste yaşıyorsunuz? (Ay)</label>
                                        <input type="number" class="form-control" id="@Html.IdFor(x => x.G14Ay)" name="@Html.NameFor(x => x.G14Ay)"  value="@Model.G14Ay" disabled>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>Doğum Tarihi<br /><br /></label>
                                        <input required type="text" class="form-control form-control-sm validate[required]" id="@Html.IdFor(x=>x.DogumTarihi)" name="@Html.NameFor(x => x.DogumTarihi)" value="@Model.DogumTarihi.ToString("dd/MM/yyyy")" disabled>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>Yaş<br /><br /></label>
                                        <input type="number" class="form-control" id="@Html.IdFor(x => x.Yas)" name="@Html.NameFor(x => x.Yas)"  value="@Model.Yas" disabled>
                                    </div>
                                    <div class="form-group col-md-3">
                                        <label>Son 30 gün içinde denetimli bir ortamda bulundunuz mu? (G19)</label>
                                        <select id="@Html.IdFor(x => x.G19Soru)" name="@Html.NameFor(x => x.G19Soru)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)G19.Bir)">Hayır</option>
                                            <option value="@((int)G19.Iki)">Cezaevi</option>
                                            <option value="@((int)G19.Uc)">Alkol/Madde tedavisi</option>
                                            <option value="@((int)G19.Dort)">Tıbbi tedavi</option>
                                            <option value="@((int)G19.Bes)">Psikiyatrik tedavi</option>
                                            <option value="@((int)G19.Alti)">Diğer</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label>G19 Diğer Açıklama:<br /><br /></label>
                                        @Html.TextAreaFor(x => x.G19Aciklama, new { @class = "form-control form-control-sm validate[required]", maxlength = 500, @readonly = true })
                                    </div>
                                    <div class="form-group col-md-3">
                                        <label>Kaç gün? (G19 cevabı Hayır ise "HH" yazınız.)</label>
                                        <input type="text" class="form-control" id="@Html.IdFor(x => x.G20Soru)" name="@Html.NameFor(x => x.G20Soru)" maxlength="10"  value="@Model.G20Soru" disabled>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label>Sevk kaynağı: Sizi tedaviye kim yönlendirdi?</label>
                                        @Html.TextAreaFor(x => x.SevkKaynagi, new { @class = "form-control form-control-sm validate[required]", maxlength = 500, @readonly = true })
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label>GENEL BİLGİ YORUMLARI</label>
                                        @Html.TextAreaFor(x => x.GenelBilgiKaynagi, new { @class = "form-control form-control-sm validate[required]", maxlength = 500, @readonly = true })
                                    </div>
                                </div>
                                <h5>Tıbbi Durum</h5>
                                <div class="form-row">
                                    <div class="form-group col-md-3">
                                        <label>
                                            Hayatınızda kaç kez tıbbi sorunlar nedeniyle
                                            hastaneye yatırıldınız?
                                        </label>
                                        <input type="number" class="form-control" id="@Html.IdFor(x => x.M1Soru)" name="@Html.NameFor(x => x.M1Soru)"  value="@Model.M1Soru" disabled>
                                    </div>
                                    <div class="form-group col-md-3">
                                        <label>
                                            Hayatınızı etkilemeye devam eden kronik tıbbi sorunlarınız var mı? (M3)
                                        </label>
                                        <select id="@Html.IdFor(x => x.M3Soru)" name="@Html.NameFor(x => x.M3Soru)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)EvetHayir.Evet)">Evet</option>
                                            <option value="@((int)EvetHayir.Hayir)">Hayır</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>(M3) Evet Açıklama:<br /><br /></label>
                                        @Html.TextAreaFor(x => x.M3SoruAciklama, new { @class = "form-control form-control-sm validate[required]", maxlength = 500, @readonly = true })
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label>
                                            Bir sağlık hizmeti sağlayıcısı,
                                            fiziksel bir sorun için düzenli olarak
                                            herhangi bir ilaç almanızı önerdi mi?
                                        </label>
                                        <select id="@Html.IdFor(x => x.M4Soru)" name="@Html.NameFor(x => x.M4Soru)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)EvetHayir.Evet)">Evet</option>
                                            <option value="@((int)EvetHayir.Hayir)">Hayır</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>
                                            Fiziksel bir engellilik için parasal destek alıyor musunuz? (M5)
                                        </label>
                                        <select id="@Html.IdFor(x => x.M5Soru)" name="@Html.NameFor(x => x.M5Soru)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)EvetHayir.Evet)">Evet</option>
                                            <option value="@((int)EvetHayir.Hayir)">Hayır</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>(M5) Evet Açıklama:<br /><br /><br /></label>
                                        @Html.TextAreaFor(x => x.M5SoruAciklama, new { @class = "form-control form-control-sm validate[required]", maxlength = 500, @readonly = true })
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>Son 30 gün içinde kaç gün tıbbi sorunlar yaşadınız?<br /><br /></label>
                                        <input type="number" class="form-control" id="@Html.IdFor(x => x.M6Soru)" name="@Html.NameFor(x => x.M6Soru)"  value="@Model.M6Soru" disabled>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>
                                            Son 30 günde, bu tıbbi sorunlardan ne kadar sıkıntı duydunuz?
                                        </label>
                                        <select id="@Html.IdFor(x => x.M7Soru)" name="@Html.NameFor(x => x.M7Soru)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)HastaDegerlendirmeOlcegi.Sifir)">Hiç Değil</option>
                                            <option value="@((int)HastaDegerlendirmeOlcegi.Bir)">Biraz</option>
                                            <option value="@((int)HastaDegerlendirmeOlcegi.Iki)">Orta derecede</option>
                                            <option value="@((int)HastaDegerlendirmeOlcegi.Uc)">Önemli ölçüde</option>
                                            <option value="@((int)HastaDegerlendirmeOlcegi.Dort)">Son derece</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>
                                            Bu tıbbi sorunların tedavisi sizin için şimdi n e kadar önemli? (M8)
                                        </label>
                                        <select id="@Html.IdFor(x => x.M8Soru)" name="@Html.NameFor(x => x.M8Soru)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)HastaDegerlendirmeOlcegi.Sifir)">Hiç Değil</option>
                                            <option value="@((int)HastaDegerlendirmeOlcegi.Bir)">Biraz</option>
                                            <option value="@((int)HastaDegerlendirmeOlcegi.Iki)">Orta derecede</option>
                                            <option value="@((int)HastaDegerlendirmeOlcegi.Uc)">Önemli ölçüde</option>
                                            <option value="@((int)HastaDegerlendirmeOlcegi.Dort)">Son derece</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>(M8) Açıklama:<br /><br /><br /></label>
                                        @Html.TextAreaFor(x => x.M8SoruAciklama, new { @class = "form-control form-control-sm validate[required]", maxlength = 500, @readonly = true })
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>Hastanın yanlış beyanı?<br /><br /></label>
                                        <select id="@Html.IdFor(x => x.M10Soru)" name="@Html.NameFor(x => x.M10Soru)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)EvetHayir.Evet)">Evet</option>
                                            <option value="@((int)EvetHayir.Hayir)">Hayır</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>Hastanın anlayamaması?<br /><br /></label>
                                        <select id="@Html.IdFor(x => x.M11Soru)" name="@Html.NameFor(x => x.M11Soru)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)EvetHayir.Evet)">Evet</option>
                                            <option value="@((int)EvetHayir.Hayir)">Hayır</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-3">
                                        <label>Hiç hepatit testi yaptırdınız mı?<br /><br /></label>
                                        <select id="@Html.IdFor(x => x.M12Soru)" name="@Html.NameFor(x => x.M12Soru)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)EvetHayir.Evet)">Evet</option>
                                            <option value="@((int)EvetHayir.Hayir)">Hayır</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-2">
                                        <label>Evet ise, sonuç ne idi?<br /><br /></label>
                                        <select id="@Html.IdFor(x => x.M12aSoru)" name="@Html.NameFor(x => x.M12aSoru)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)TestSonuc.Bir)">Negatif (enfekte değil)</option>
                                            <option value="@((int)TestSonuc.Iki)">Pozitif (enfekte)</option>
                                            <option value="@((int)TestSonuc.Uc)">Bilmiyor</option>
                                            <option value="@((int)TestSonuc.Dort)">Hayır</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-3">
                                        <label>Hepatit testi yaptırmak için yardım ister misiniz?</label>
                                        <select id="@Html.IdFor(x => x.M12bSoru)" name="@Html.NameFor(x => x.M12bSoru)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)EvetHayir.Evet)">Evet</option>
                                            <option value="@((int)EvetHayir.Hayir)">Hayır</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label>Hiç HIV testi yaptırdınız mı?</label>
                                        <select id="@Html.IdFor(x => x.M13Soru)" name="@Html.NameFor(x => x.M13Soru)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)EvetHayir.Evet)">Evet</option>
                                            <option value="@((int)EvetHayir.Hayir)">Hayır</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label>Evet ise, sonuç ne idi?</label>
                                        <select id="@Html.IdFor(x => x.M13aSoru)" name="@Html.NameFor(x => x.M13aSoru)" class="form-control">
                                            <option selected>Seçiniz</option>
                                            <option value="@((int)TestSonuc.Bir)">Negatif (enfekte değil)</option>
                                            <option value="@((int)TestSonuc.Iki)">Pozitif (enfekte)</option>
                                            <option value="@((int)TestSonuc.Uc)">Bilmiyor</option>
                                            <option value="@((int)TestSonuc.Dort)">Hayır</option>
                                        </select>
